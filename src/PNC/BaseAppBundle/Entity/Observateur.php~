<?php

namespace PNC\BaseAppBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Observateur
 */
class Observateur
{
    /**
     * @var integer
     */
    private $obs_id;

    /**
     * @var string
     */
    private $nom;


    /**
     * Get obs_id
     *
     * @return integer 
     */
    public function getObsId()
    {
        return $this->obs_id;
    }

    /**
     * Set nom
     *
     * @param string $nom
     * @return Observateur
     */
    public function setNom($nom)
    {
        $this->nom = $nom;

        return $this;
    }

    /**
     * Get nom
     *
     * @return string 
     */
    public function getNom()
    {
        return $this->nom;
    }
}
